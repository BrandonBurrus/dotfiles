{
  "prettier.requireConfig": true,

  "diagnostic.level": "hint",
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",

  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "prettier.disableSuccessMessage": true,
  "yank.highlight.duration": 110,
  "css.colorDecorators.enable": true,
  "scss.colorDecorators.enable": true,
  "scss.validate": false,
  "tailwindCSS.emmetCompletions": true,

  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": [
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ]
  },

  "sourcekit.commandPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
  "sourcekit.iOSsdkPath": "/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator14.1.sdk",

  "typescript.suggest.autoImports": true,
  "typescript.suggest.enabled": true,

  "cSpell.userWords": [
    "Algolia",
    "Autosave",
    "Burrus",
    "CKAD",
    "Codesandbox",
    "Coursera",
    "Eudaimonia",
    "GRADESCALE",
    "Gblame",
    "Gcommit",
    "Gvdiff",
    "HATEOAS",
    "Importants",
    "JNLP",
    "Keras",
    "Kubernetes",
    "Liquibase",
    "Maplewood",
    "Mitigations",
    "Mockito",
    "Ngrinx",
    "OWASP",
    "Postgres",
    "Pytest",
    "SDLC",
    "Todos",
    "Ulti",
    "Vuex",
    "blinkon",
    "chakra",
    "codebases",
    "debuggable",
    "desync",
    "fullscreen",
    "gradebook",
    "grpc",
    "gutentags",
    "immer",
    "omnisharp",
    "performant",
    "prettierrc",
    "refactorable",
    "scrollable",
    "scrollbar",
    "scrollbars",
    "scroller",
    "semibold",
    "startify",
    "testid",
    "tpope",
    "trigged",
    "typecheck",
    "unmount",
    "vetur",
    "vrfr"
  ]

}
