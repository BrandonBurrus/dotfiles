{
  "prettier.requireConfig": true,
  "diagnostic.errorSign": "❗️",
  "diagnostic.warningSign": "🔸",
  "diagnostic.infoSign": "🔹",
  "diagnostic.hintSign": "💡",

  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "prettier.disableSuccessMessage": true,
  "yank.highlight.duration": 110,
  "css.colorDecorators.enable": true,
  "scss.colorDecorators.enable": true,
  "scss.validate": false,
  "tailwindCSS.emmetCompletions": true,

  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": [
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ]
  },

  "sourcekit.commandPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",

  "typescript.suggest.autoImports": true,
  "typescript.suggest.enabled": true,

  "cSpell.userWords": [
    "GRADESCALE",
    "Gblame",
    "Gcommit",
    "Gvdiff",
    "Ulti",
    "blinkon",
    "chakra",
    "gradebook",
    "gutentags",
    "omnisharp",
    "prettierrc",
    "semibold",
    "startify",
    "testid",
    "unmount",
    "vetur",
    "vrfr"
  ]

}
