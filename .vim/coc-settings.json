{
  "prettier.requireConfig": true,

  "diagnostic.level": "hint",
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",

  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "prettier.disableSuccessMessage": true,
  "yank.highlight.duration": 110,
  "css.colorDecorators.enable": true,
  "scss.colorDecorators.enable": true,
  "scss.validate": false,
  "tailwindCSS.emmetCompletions": true,

  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": [
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ]
  },

  "sourcekit.commandPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
  "sourcekit.sdkPath": "/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator14.1.sdk",

  "java.jdt.ls.vmargs": "-javaagent:/Users/brandonburrus/.m2/repository/org/projectlombok/lombok/1.18.16/lombok-1.18.16.jar",
  "java.codeGeneration.useBlocks": true,
  "java.implementationsCodeLens.enabled": true,
  "java.format.onType.enabled": true,
  "java.referencesCodeLens.enabled": true,
  "java.format.onType.fixComment.enabled": true,
  "java.codeGeneration.toString.codeStyle": "STRING_FORMAT",

  "typescript.suggest.autoImports": true,
  "typescript.suggest.enabled": true,

  "bookmark.sign": "",

  "explorer.width": 45,
  "explorer.icon.enableVimDevicons": true,
  "explorer.previewAction.onHover": false,
  "explorer.presets": {
    "base": {
      "sources": [
        {
          "name": "file",
          "expand": true
        }
      ],
      "file-root-template": "  [git & 1][hidden & 1][root]",
      "file-child-template": "[selection | clip | 1] [indent][icon | 1] [filename omitCenter 1]"
    }
  },

